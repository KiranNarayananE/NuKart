	<script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.0/axios.min.js"></script>
	<script>
		
		function wishList(productId){
			axios.post('/wishList/'+productId)
    .then(response => {
		if(!response.data.status){
		swal("SUCCESS"," Removed from WishList!","error")
        document.getElementById(productId).style.display="none"	
		}
		else if(msg_login){
			location.href="/"
		}	
		})
		}
	</script>
    <script>
     
      function addtoCart(productId){
      let url = `/cart?id=${productId}`
	  
      axios.post(url,{quantity:1}
     ).then(response=>{
        console.log(response.data)
      if(response.data.msg_outOfStock){
        swal("SORRY","Product is Out of Stock!","error")
      }
      if(response.data.msg_quantityOfStock){
        swal("SORRY","Quantity not Available with Seller! Try Reducing Quantity","error")
      }
      if(response.data.msg_login){
       
         swal("LOGIN","Please Login To Add to Cart ","info")
	
      }
      if(response.data.msg_addToCartSuccess){
       swal("SUCCESS"," Product is Added to Cart!","success") 
      }

      
     })
     }
     
     
    </script>
